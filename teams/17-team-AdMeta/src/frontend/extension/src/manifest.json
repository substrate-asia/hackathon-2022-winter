{
  "name": "Hackathon AdMeta",
  "version": "0.0.1",
  "description": "A privacy-preserving Ad Platform in Metaverse",
  "homepage_url": "https://admeta.network/",
  "manifest_version": 2,
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "background": {
    "scripts": [
      "js/background.ts"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content-script.ts"
      ]
    }
  ],
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "assets/16.png",
      "48": "assets/48.png"
    },
    "default_title": "AdMeta",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "clipboardRead",
    "clipboardWrite"
  ]
}